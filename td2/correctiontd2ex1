char 1
int 4
double 8
char * {
void *
int * 		4 ou 8
double *
int **       }
int [10] 40 
char [7][3] 21
int [] --> erreur de compilation

int tab [] = {1,2,3}

char tab [10] 10
tab 1
tab[0] 
& tab [0] 4 ou 8 car adresse
* & tab 10
* & tab [0] 1

char (*p) [10] = &tab

(*p) [0] --> char
*p --> tableau de char (taille 10)
p --> pointeur sur tableau de char


p : 4 ou 8
*p : 10
(*p)[2] 1
& (*p)[2] 4 ou 8

